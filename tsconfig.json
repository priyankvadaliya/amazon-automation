{
    "compilerOptions": {
      "target": "ES2019",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "*": ["*", "pages/*"]
      }
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules"]
  }